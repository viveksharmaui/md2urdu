{
  "name": "md2urdu",
  "displayName": "md2urdu",
  "publisher": "vivek",
  "description": "converts any text in .md file to urdu",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.md2urdu"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.md2urdu",
        "title": "converts any text in .md file to urdu"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {},
  "dependencies": {
    "@vitalets/google-translate-api": "^3.0.0",
    "typescript": "^3.3.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.2",
    "snyk": "^1.230.5"
  },
  "snyk": true
}
